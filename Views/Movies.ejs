    <body>
        <h1>Seach Result <span></span>AVENGERS</h1>
        <div class="card">
            <div class="row no-gutters">
                <div class="col-md-4">
                    <img src="" class="card-img img-fluid">
                </div>
                <div class="col-md-8">
                    <div class="card-body">
                        <h2 class="card-title">Avengers</h2>
                        <hr>
                        <p class="card-text">Summary of movie goes here</p>
                        <hr>
                        <p class="card-text"><b>Date Published :</b> 2019</p>
                        <p class="card-text"><b>Total Votes :</b> 20</p>
                        <p class="card-text"><b>Average rating :</b> 5.5</p>
                    </div>
                </div>
            </div>
        </div>
